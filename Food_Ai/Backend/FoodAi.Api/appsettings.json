{
  "ConnectionStrings": {
    "Default": "Data Source=foodai.db"
  },
  "Jwt": {
    "Issuer": "FoodAi",
    "Audience": "FoodAi.Users",
    "Secret": "",
    "ExpiresMinutes": 60
  },
  "Spoonacular": {
    "ApiKey": "",
    "BaseUrl": "https://api.spoonacular.com"
  },
  "Cors": {
    "Origin": "http://localhost:8080"
  },
  "RateLimiting": {
    "RecipeSearch": {
      "PermitLimit": 10,
      "WindowSeconds": 60
    }
  },
  "Cache": {
    "RecipeHours": 24
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
